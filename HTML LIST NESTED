<ul>
      <li> General Data </li>
      <li>
         <!-- We put another list inside an <li> element. -->
         <ul>
            <li> Specific Data </li>
            <li> Specific Data </li>
            <li> Specific Data </li>
         </ul>
      </li>
      <li> General Data </li>
      <li>
         <ol>
            <li> Specific Data </li>
            <li> Specific Data </li>
            <li> Specific Data </li>
         </ol>
      </li>
   <ul/>
